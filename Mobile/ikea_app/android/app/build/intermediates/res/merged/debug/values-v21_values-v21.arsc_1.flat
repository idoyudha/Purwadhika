AA,sBAFA,eAaA,QAAC,WAAD;AAAa,gBAAA,SAAS,EAAC,gCAAvB;AAAA,wCACI,QAAC,MAAD;AAAQ,kBAAA,SAAS,EAAC,+BAAlB;AAAkD,kBAAA,OAAO,MAAzD;AAA0D,kBAAA,KAAK,EAAC,WAAhE;AAA4E,kBAAA,OAAO,EAAE,KAAKT,MAA1F;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,wBADJ,eAEI,QAAC,MAAD;AAAQ,kBAAA,SAAS,EAAC,+BAAlB;AAAkD,kBAAA,KAAK,EAAC,SAAxD;AAAkE,kBAAA,OAAO,EAAE,MAAM,KAAKc,SAAL,CAAeW,IAAI,CAACV,IAApB,EAA0BU,IAAI,CAACT,GAA/B,EAAoCS,IAAI,CAACR,KAAzC,CAAjF;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,wBAFJ,EAEoK,GAFpK;AAAA;AAAA;AAAA;AAAA;AAAA,sBAbA;AAAA;AAAA;AAAA;AAAA;AAAA,oBALJ;AAAA;AAAA;AAAA;AAAA;AAAA,kBAFJ;AAAA;AAAA;AAAA;AAAA;AAAA;AADA;AAAA;AAAA;AAAA;AAAA,cAAR;AA6BH,KA9BM,CAAP;AA+BH;;AAED2B,EAAAA,MAAM,GAAG;AACLvB,IAAAA,OAAO,CAACC,GAAR,CAAY,OAAZ,EAAqB,KAAKvB,KAA1B;;AACA,QAAI,KAAKO,KAAL,CAAWF,QAAX,IAAuB,IAA3B,EAAiC;AAC7B,0BAAO,QAAC,QAAD;AAAU,QAAA,EAAE,EAAC;AAAb;AAAA;AAAA;AAAA;AAAA,cAAP;AACH;;AACD,wBACI;AAAK,MAAA,SAAS,EAAC,iBAAf;AAAA,8BACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cADJ,eAEI;AAAA,+BACA;AAAK,UAAA,KAAK,EAAE;AAAC+B,YAAAA,OAAO,EAAE;AAAV,WAAZ;AAA+B,UAAA,QAAQ,EAAEU,EAAE,IAAI,KAAKtC,KAAL,GAAasC,EAA5D;AAAA,iCACI,QAAC,KAAD;AAAO,YAAA,MAAM,EAAE,KAAKvC,KAAL,CAAWC,KAA1B;AAAA,oCACI,QAAC,WAAD;AAAa,cAAA,IAAI,EAAC,SAAlB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,oBADJ,eAII,QAAC,SAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,oBAJJ;AAAA;AAAA;AAAA;AAAA;AAAA;AADJ;AAAA;AAAA;AAAA;AAAA;AADA;AAAA;AAAA;AAAA;AAAA,cAFJ,eAcI;AAAK,QAAA,SAAS,EAAC,KAAf;AAAA,kBACK,KAAKgC,SAAL;AADL;AAAA;AAAA;AAAA;AAAA,cAdJ;AAAA;AAAA;AAAA;AAAA;AAAA,YADJ;AAqBH;;AAnJ4B;;AAsJjC,MAAMO,eAAe,GAAG,CAAC;AAAEC,EAAAA,eAAF;AAAmBC,EAAAA;AAAnB,CAAD,KAAsC;AAC1D,SAAO,EACH,GAAGD,eADA;AAEH,OAAGC;AAFA,GAAP;AAIH,CALD;;AAOA,eAAetE,OAAO,CAACoE,eAAD,EAAkB;AAACnE,EAAAA;AAAD,CAAlB,CAAP,CAA6CkB,QAA7C,CAAf","sourcesContent":["import React, { Component } from 'react';\r\nimport { URL_API } from '../helper';\r\nimport axios from 'axios';\r\nimport { connect } from 'react-redux';\r\nimport { getProductAction } from '../action/productAction'\r\nimport { Link, Redirect } from 'react-router-dom';\r\nimport {\r\n    Card, CardImg, CardText, CardBody, Modal, Input, Toast, ToastBody, ToastHeader,\r\n    CardTitle, CardSubtitle, Button, ModalHeader, ModalBody, ModalFooter\r\n  } from 'reactstrap';\r\n\r\n\r\nclass HomePage extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {  \r\n            modal: false,\r\n            count: 1,\r\n            redirect: false,\r\n            toast: false\r\n        }\r\n    }\r\n\r\n    componentDidMount() {\r\n        this.getProducts()\r\n    }\r\n\r\n    getProducts() {\r\n        axios.get(URL_API+`/products`)\r\n        .then(response => {\r\n            console.log(\"product list\", response.data)\r\n            this.props.getProductAction(response.data)\r\n        })\r\n        .catch(error => {\r\n            console.log(error)\r\n        })\r\n    }\r\n\r\n    toggle = () => {\r\n        if (this.props.id === null) {\r\n            alert('Login first')\r\n            this.setState({redirect: true})\r\n        }\r\n        else {\r\n            this.setState({ modal: !this.state.modal })\r\n            this.setState({ toast: !this.state.toast })\r\n        }\r\n    }\r\n\r\n    add = (index) => {\r\n        if (this.state.count < this.props.product_list[index].stock) {\r\n            this.setState(state => ({\r\n                count: state.count + 1\r\n            }))\r\n        }\r\n        else {\r\n            alert('Stock not enough')\r\n        }\r\n    }\r\n\r\n    remove = () => {\r\n        if (this.state.count > 1 ) {\r\n            this.setState({count: this.state.count - 1})\r\n        }\r\n    }\r\n\r\n    addtoCart = (name, img, price) => {\r\n        let userID = this.props.id\r\n        let qty = this.state.count\r\n        let subtotal = qty * price\r\n        console.log('Add to Cart', userID, name, img, qty, subtotal)\r\n        let cart = this.props.cart\r\n        let index = cart.findIndex(item => item.name === name)\r\n        console.log('Findindex', index)\r\n        if (index > -1) {\r\n            cart[index].qty += qty\r\n        }\r\n        else {\r\n            cart.push({\r\n                name, img, qty, subtotal\r\n            })\r\n        }\r\n        axios.patch(URL_API+`/users/${userID}`, {\r\n            cart: this.props.cart\r\n        })\r\n        .then(response => {\r\n            console.log(response.data)\r\n            \r\n        })\r\n        .catch(error => {\r\n            console.log(\"Error\", error)\r\n        })\r\n        this.toggle()\r\n    }\r\n\r\n    handleToast = () => {\r\n        this.toast.style.display = 'block'\r\n    }\r\n\r\n    printCard() {\r\n        return this.props.product_list.map((item, index) => {\r\n            return  <div className=\"col-md-3 col-6\">\r\n                    <Card>\r\n                        <CardImg width=\"100\" src={item.img} alt=\"Card image cap\" />\r\n                        <CardBody>\r\n                        <CardTitle tag=\"h5\">{item.name}</CardTitle>\r\n                        <CardSubtitle tag=\"h6\" className=\"mb-2 text-muted\">Price: IDR {item.price}</CardSubtitle>\r\n                        <CardSubtitle tag=\"h6\" className=\"mb-2 text-muted\">Stock: {item.stock}</CardSubtitle>\r\n                            <Button color=\"warning\" block onClick={this.toggle}>Add to cart</Button>\r\n                            <Modal size=\"lg\" isOpen={this.state.modal} toggle={this.toggle} style={{width: '70%'}}>\r\n                            <ModalHeader toggle={this.toggle} charCode=\"x\">Add {item.name} to cart, define quantity</ModalHeader>\r\n                            <ModalBody>\r\n                            <div className=\"d-flex justify-content-between align-items-center\">\r\n                            <img src={item.img} style={{maxWidth: '100px'}}></img>\r\n                            <span>Total qty:</span>\r\n                                <span style={{width: '30%', display: 'flex', alignItems:'center'}}>\r\n                                    <Button outline size=\"sm\" onClick={() => this.remove(index)}>remove</Button>\r\n                                    <Input className=\"text-center\" placeholder='qty' value={this.state.count} style={{width: '50%', display:'inline-block'}} />\r\n                                    <Button outline size=\"sm\" color=\"primary\" onClick={() => this.add(index)}>add</Button>\r\n                                </span>\r\n                            </div>\r\n                            </ModalBody>\r\n                            <ModalFooter className=\"d-flex justify-content-between\">\r\n                                <Button className=\"d-flex justify-content-center\" outline color=\"secondary\" onClick={this.toggle}>Continue Shopping</Button>\r\n                                <Button className=\"d-flex justify-content-center\" color=\"warning\" onClick={() => this.addtoCart(item.name, item.img, item.price)}>Add to shopping cart</Button>{' '}\r\n                            </ModalFooter>\r\n                            </Modal>\r\n                        </CardBody>\r\n                    </Card>\r\n                    </div>\r\n        })\r\n    }\r\n\r\n    render() { \r\n        console.log(\"props\", this.props)\r\n        if (this.state.redirect == true) {\r\n            return <Redirect to=\"/login\" />\r\n        }\r\n        return (  \r\n            <div className=\"container-fluid\">\r\n                <h1>Homepage</h1>\r\n                <div>\r\n                <div style={{display: 'none'}} innerRef={el => this.toast = el}>\r\n                    <Toast isOpen={this.state.toast}>\r\n                        <ToastHeader icon=\"success\">\r\n                            Successfully adding to cart\r\n                        </ToastHeader>\r\n                        <ToastBody>\r\n                            Just one step closer!\r\n                        </ToastBody>\r\n                    </Toast>\r\n                </div>\r\n                </div>\r\n           